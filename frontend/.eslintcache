tatus === 201) {\n        setIsSuccessfullySubmitted(true)\n      }\n    })\n  }\n\n  function inputHandler(e) {\n    const newData = { ...data }\n    newData[e.target.id] = e.target.value\n    setData(newData)\n  }\n\n  return (\n    <form onSubmit={(e) => submitHandler(e)}>\n      <Col>\n        <Row>\n          <Col md={6}>\n            <InputWithLabel\n              required\n              htmlFor='name'\n              label='Махсулот Номи'\n              placeholder='Махсулот Номи'\n              type='text'\n              name='name'\n              id='name'\n              value={data.name}\n              onChange={(e) => inputHandler(e)}\n              disabled={isSuccessfullySubmitted ? true : false}\n            />\n          </Col>\n          <Col md={6}>\n            <InputDropdown\n              required\n              htmlFor='supplier'\n              label='Таъминотчи'\n              placeholder='Таъминотчи'\n              name='supplier'\n              id='supplier'\n              value={data.supplier}\n              onChange={(e) => inputHandler(e)}\n              items={supplierList}\n              disabled={isSuccessfullySubmitted ? true : false}\n            />\n          </Col>\n\n          <Col md={6}>\n            <InputDropdown\n              required\n              htmlFor='brand'\n              label='Махсулот Марка'\n              placeholder='Махсулот Марка'\n              name='brand'\n              id='brand'\n              value={data.brand}\n              onChange={(e) => inputHandler(e)}\n              items={brandList}\n              disabled={isSuccessfullySubmitted ? true : false}\n            />\n          </Col>\n\n          <Col md={6}>\n            <InputDropdown\n              required\n              htmlFor='category'\n              label='Махсулот Тоифаси'\n              placeholder='Махсулот Тоифаси'\n              name='category'\n              id='category'\n              value={data.category}\n              onChange={(e)